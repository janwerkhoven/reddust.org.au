{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "$": true,
    "ga": true,
    "CountUp": true
  },
  "rules": {
    "no-debugger": 1,
    "no-console": 1,
    "no-unused-vars": 1,
    "prettier/prettier": "error"
  }
}
